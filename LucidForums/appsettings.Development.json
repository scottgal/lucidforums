{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext" ],
    "Properties": { "Application": "LucidForums-Dev" }
  },
  "Otlp": {
    "Endpoint": "http://localhost:4317"
  },
  "Service": {
    "Name": "LucidForums",
    "Version": "dev"
  }
}
