{
 

  "scripts": {
 

   

  "dev": "npm-run-all --parallel dev:*",
 

   

  "dev:js": "webpack",
 

   

  "dev:tw": "npx tailwindcss -i ./src/css/main.css -o ./wwwroot/css/dist/main.css",
 

   

  "watch": "npm-run-all --parallel watch:*",
 

   

  "watch:js": "webpack --watch --env development",
 

   

  "watch:tw": "npx tailwindcss -i ./src/css/main.css -o ./wwwroot/css/dist/main.css --watch",
 

   

  "build": "npm-run-all --parallel build:*",
 

   

  "build:js": "webpack --env production",
 

   

  "build:tw": "npx tailwindcss -i ./src/css/main.css -o ./wwwroot/css/dist/main.css --minify"
 

  },
 

  "devDependencies": {
 

   

  "@babel/core": "7.26.10",
 

   

  "@babel/preset-env": "7.26.9",
 

   

  "@tailwindcss/aspect-ratio": "^0.4.2",
 

   

  "autoprefixer": "^10.4.20",
 

   

  "babel-loader": "10.0.0",
 

   

  "core-js": "^3.38.1",
 

   

  "css-loader": "7.1.2",
 

   

  "daisyui": "^4.12.10",
 

   

  "npm-run-all": "^4.1.5",
 

   

  "style-loader": "4.0.0",
 

   

  "tailwindcss": "3.4.17",
 

   

  "terser-webpack-plugin": "^5.3.10",
 

   

  "webpack": "^5.91.0",
 

   

  "webpack-cli": "^5.1.4"
 

  },
 

  "dependencies": {
 

   

  "@microsoft/signalr": "^9.0.6",
 

   

  "alpinejs": "3.14.9",
 

   

  "easymde": "^3.0.0-alpha.4.0",
 

   

  "flatpickr": "4.6.13",
 

   

  "highlight.js": "11.11.1",
 

   

  "htmx.org": "2.0.4",
 

   

  "sweetalert2": "11.17.2"
 

  }
}
